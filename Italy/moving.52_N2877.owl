        <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E39_Actor"/>
        <rdfs:range rdf:resource="http://erlangen-crm.org/current/E5_Event"/>
        <rdfs:label xml:lang="en">P11 participated in</rdfs:label>
