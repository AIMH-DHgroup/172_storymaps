        <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E2_Temporal_Entity"/>
        <rdfs:range rdf:resource="http://erlangen-crm.org/current/E2_Temporal_Entity"/>
        <rdfs:label xml:lang="en">P117 includes</rdfs:label>
