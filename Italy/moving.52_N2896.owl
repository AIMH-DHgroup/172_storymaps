        <current:P7_took_place_at rdf:resource="https://dlnarratives.eu/phenomenal_place/45.866666666_11.516666666"/>
        <current:P7_took_place_at rdf:resource="https://dlnarratives.eu/place/91"/>
        <ontology:hasEntity rdf:resource="https://dlnarratives.eu/resource/Q11004"/>
        <ontology:hasEntity rdf:resource="https://dlnarratives.eu/resource/Q131596"/>
        <ontology:hasEntity rdf:resource="https://dlnarratives.eu/resource/Q174165"/>
        <ontology:hasEntity rdf:resource="https://dlnarratives.eu/resource/Q2920921"/>
